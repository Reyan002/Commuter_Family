<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.commuterfamily">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"

android:icon="@mipmap/icon_image_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_image_new"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Main2Activity"></activity>
        <activity android:name=".Activities.CFSignUp" />
        <activity android:name=".Activities.CFLoginActivity" />
        <activity android:name=".SplashScreenActivity" />
        <activity
            android:name=".Activities.LoginSignupOptionActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>