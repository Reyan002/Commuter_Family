<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textview1_adapterMI"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:drawableLeft="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:textSize="14dp" />


        <TextView
            android:id="@+id/textview2_adapterMI"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:drawableLeft="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/textview3_adapterMI"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:drawableLeft="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/textview4_adapterMI"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:drawableLeft="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/textview5_adapterMI"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:drawableLeft="@drawable/ic_keyboard_arrow_right_black_24dp"
            android:textSize="14dp" />
    </LinearLayout>

</androidx.cardview.widget.CardView>